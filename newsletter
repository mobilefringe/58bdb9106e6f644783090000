<script>
    $(document).ready(function(e){
        init(e);
        
        function renderAll(){
            var repo = getRepoDetailsByName("Newsletter").images;
            if(repo != null){
                renderRepo("#newsletter_image_container", "#newsletter_image_template", repo);
            }
            
            $.validate();

            show_content();
        }
        
        loadMallDataCached(renderAll); 
    });
