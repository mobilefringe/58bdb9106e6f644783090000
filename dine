<script>
    $(document).ready(function(e){
        init(e);
        function renderAll(){
            var dining = getStoresListByCategoryID(5034);
            var late_night = getStoresListByCategoryID(5035);
            var rock_rose = getStoresListByCategoryID(5032);
            var specialty_foods = getStoresListByCategoryID(5039);

            var dine_list = dining.concat(late_night, rock_rose, specialty_foods).sortBy(function(o){ return o.name});
            var sorted_list = _.uniq(dine_list, function(o){ return o.name; });
            console.log(sorted_list)
            renderStoreList('#store_list_container','#store_list_template', sorted_list, "stores");
            
            show_content();
            
            store_search();
            show_cat_stores();
            $('#top_nav_stores').addClass('active_yellow')
        }
        loadMallDataCached(renderAll); 
    });
</script>